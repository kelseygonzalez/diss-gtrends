@article{myhreImpactWidelyPublicized2021,
  title = {The {{Impact}} of a {{Widely Publicized Celebrity Suicide}} on {{Suicide-Related Internet Search Activity}}},
  author = {Myhre, Martin Ø. and Walby, Fredrik A.},
  date = {2021-01-27},
  journaltitle = {Archives of Suicide Research},
  shortjournal = {Archives of Suicide Research},
  pages = {1--7},
  issn = {1381-1118, 1543-6136},
  doi = {10.1080/13811118.2021.1875942},
  url = {https://www.tandfonline.com/doi/full/10.1080/13811118.2021.1875942},
  urldate = {2021-02-02},
  abstract = {Objective: Media reporting of celebrities’ deaths by suicide are prone to suicide contagion effects. The aim of the current study is to examine whether the widely publicized celebrity suicide of Ari Behn in Norway was associated with changes in search activity of suiciderelated terms. Method: Search activity data for the terms “suicide,” “Ari Behn suicide,” “how to suicide,” and “suicide prevention” were retrieved from Google Trends. We analyzed data as an interrupted time series and used T-tests to compare means before and after the suicide. Crude linear models examining the association between searches for “suicide” over time and an adjusted model controlling for searches after “Ari Behn suicide” were built. The models were tested with structural change tests. Results: A significant increase in search activity for “suicide” (p ¼ {$<$} .001), “Ari Behn suicide” (p ¼ .002), and “how to suicide” (p ¼ .006) was found after the suicide. Searches for “suicide prevention” were not significant (p ¼ .11). The structural change test was significant both for the model that did not control for explicit searches (p ¼ {$<$}.001) and for the model controlled for explicit searches (p ¼ {$<$}.001). Conclusions: A recent widely publicized suicide in Norway was associated with increases in Google searches for suicide. No indications of the Papageno effect were found. The media should be cautious when reporting about the suicides of prominent public persons. Compliance with generally accepted media reporting guidelines may need more attention.},
  langid = {english},
  file = {C\:\\Users\\ckgon\\Zotero\\storage\\GHDLZ3Q6\\Myhre and Walby - 2021 - The Impact of a Widely Publicized Celebrity Suicid.pdf}
}

@report{nazarChangesAirpollutionRelated2021,
  type = {preprint},
  title = {Changes in Air-Pollution Related Behaviour Measured by {{Google Trends Search Volume Index}} in Response to Reported Air Quality in {{Poland}}},
  author = {Nazar, Wojciech and Plata-Nazar, Katarzyna},
  date = {2021-02-15},
