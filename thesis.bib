@article{johnsonNowcastingSexuallyTransmitted2020,
  ids = {johnson_etal20b},
  title = {Nowcasting {{Sexually Transmitted Infections}} in {{Chicago}}: {{Predictive Modeling}} and {{Evaluation Study Using Google Trends}}},
