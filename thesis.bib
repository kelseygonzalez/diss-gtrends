@article{seaverNiceThingContext2015,
  title = {The Nice Thing about Context Is That Everyone Has It},
  author = {Seaver, Nick},
  date = {2015-10-01},
  journaltitle = {Media, Culture \& Society},
  shortjournal = {Media, Culture \& Society},
  volume = {37},
